
// Step 1: Set the correct password
var password = "12345";

// Step 2: Ask for username and password using prompt
var user_name = prompt("Enter your username:");
var user_password = prompt("Enter your password:");

// Step 3: Check if the password is correct
if (user_password === password) {
    alert(`Welcome ${user_name}`);
} else {
    alert("Maybe Username or Password is Invalid or Does not Match");
}
